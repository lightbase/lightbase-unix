	doscp -r prep_porte.lb b:prep_por.lb ; echo prep_porte.lb
	doscp -r finaliza.lb b:finaliza.lb   ; echo finaliza.lb
	doscp -r mac_demo.tar b:             ; echo mac_demo.tar


