p=B:
	echo "Coloque Midia de 3 1/2 (alta densidade ed b:) para tap.Z e tct.Z ..."
	echo "Quando Ok. tecle ENTER\c"; read lixo
	doscp -r tap.Z b:   ; echo tap.Z
	doscp -r tct.Z b:   ; echo tct.Z

	echo "Coloque Midia de 3 1/2 (alta densidade ed b:) para tmt.Z, tpd.Z e outros..."
